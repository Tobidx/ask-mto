{
  "version": 2,
  "builds": [
    {
      "src": "ask-mto-genai/ask-mto-ui/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "ask-mto-genai/ask-mto-ui/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key"
  },
  "outputDirectory": "ask-mto-genai/ask-mto-ui/.next"
} 